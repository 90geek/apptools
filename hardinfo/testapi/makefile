#===============================================================================
# export variable
#===============================================================================

include $(SDK_DIR)/config/platform.mak
include $(SDK_DIR)/config/app.mak

#===============================================================================
# local variable
#===============================================================================

DRV_LIB_OUT := ../lib/$(LINK_TYPE)

APP =testapi
OBJS := testapi.o

#===============================================================================
# rules
#===============================================================================
.PHONY: all clean install uninstall

app:$(OBJS)
	$(AT)$(CC) $(LDFLAGS) -o $(APP) $(OBJS) ${LIB}

.PHONY:clean 
clean:
	-$(AT)rm -f  $(OBJS)
	-$(AT)rm -f $(APP)
	-$(AT)rm -f *~


